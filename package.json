{
  "name": "ftp-desktop",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "ng-module": "ng g m",
    "ng-module-routing": "ng g m --routing",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "tsc": "tsc",
    "start:electron": "ng build --base-href ./ && electron ./dist/app.js",
    "start:electron2": "ng build --base-href ./ && npm run electron:tsc2 && electron ./dist/ftp-desktop/app.js",
    "electron:tsc": "tsc src/app.ts --target es2022 --module es2022 --moduleResolution nodenext --lib es2015,es2016,es2017,es2022,esnext,dom --outFile ./dist/ftp-desktop",
    "electron:tsc2": "tsc --project tsconfig-electron.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.0",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/router": "^16.0.0",
    "@types/path-browserify": "^1.0.0",
    "path-browserify": "^1.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.5",
    "@angular/cli": "~16.0.5",
    "@angular/compiler-cli": "^16.0.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^20.2.5",
    "electron": "^25.1.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~5.0.2"
  }
}
